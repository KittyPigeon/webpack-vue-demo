{
  "name": "webpack-vue-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.js",
    "build": "webpack --config webpack.config.js --mode production",
    "lint": "npm run lint-eslint && npm run style-lint",
    "lint-eslint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js src && eslint src/*.js --fix",
    "style-lint": "stylelint --config .stylelintrc.js src/**/*.{less,css,scss}",
    "husky": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --config .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --config .eslintrc.js"
    ],
    "*.{css,less,scss}": [
      "stylelint --config .stylelintrc.js"
    ],
    "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@commitlint/cli": "19.2.0",
    "@commitlint/config-conventional": "19.1.0",
    "@vue/babel-preset-jsx": "^1.4.0",
    "@vue/compiler-sfc": "^3.5.13",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "element-plus": "^2.9.1",
    "eslint": "8.0.1",
    "eslint-plugin-vue": "^9.32.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "9.0.11",
    "less-loader": "^12.2.0",
    "lint-staged": "15.2.2",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.4.2",
    "style-loader": "^4.0.0",
    "stylelint": "16.2.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "36.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.8.0",
    "stylelint-less": "^3.0.1",
    "stylelint-prettier": "5.0.0",
    "unplugin-auto-import": "^0.16.1",
    "unplugin-vue-components": "^0.25.2",
    "vue": "^3.5.13",
    "vue-loader": "^17.4.2",
    "vue-template-compiler": "^2.7.16",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "jszip": "^3.10.1",
    "pinia": "^2.3.0",
    "vue-router": "^4.5.0"
  }
}
